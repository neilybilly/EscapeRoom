<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bignerdranch.android.escaperoom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RoomActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DeskActivity" />
        <activity android:name=".BookshelfActivity" />
        <activity android:name=".BookActivity" />
        <activity android:name=".RiddleActivity" />
        <activity android:name=".UnscrambleActivity" />
        <activity android:name=".SafeActivity"></activity>
    </application>

</manifest>